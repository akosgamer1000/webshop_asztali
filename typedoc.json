{
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/dist-electron/**",
    "**/dist-react/**"
  ],
  "excludeExternals": true,
  "excludePrivate": false,
  "excludeProtected": false,
  "categorizeByGroup": true,
  "categoryOrder": ["Main", "Components", "Hooks", "Utils", "*"],
  "readme": "README.md",
  "name": "WebShop Asztali Documentation",
  "includeVersion": true,
  "theme": "default",
  "plugin": ["typedoc-plugin-markdown"],
  "hideGenerator": true
} 