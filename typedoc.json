{
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/dist-electron/**",
    "**/dist-react/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.stories.tsx"
  ],
  "excludeExternals": false,
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Main", 
    "Components/Layout",
    "Components/User",
    "Components/Product",
    "Components/Order",
    "Components/Common",
    "Components", 
    "Hooks/User",
    "Hooks/Login",
    "Hooks/Product",
    "Hooks/Order",
    "Hooks",
    "Utils", 
    "Store",
    "*"
  ],
  "readme": "README.md",
  "name": "WebShop Asztali Documentation",
  "includeVersion": true,
  "theme": "default",
  "plugin": [
    "typedoc-plugin-mermaid",
    "typedoc-plugin-rename-defaults"
  ],
  "hideGenerator": true,
  "sort": ["source-order", "required-first", "kind"],
  "navigationLinks": {
    "GitHub": "https://github.com/yourusername/webshop_asztali",
    "API": "globals.html"
  },
  "sourceLinkTemplate": "https://github.com/yourusername/webshop_asztali/blob/{gitRevision}/{path}#L{line}",
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "searchInComments": true,
  "disableSources": false,
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "logLevel": "Info",
  "jsDocCompatibility": true,
  "blockTags": [
    "@file",
    "@description",
    "@version",
    "@component",
    "@fileoverview",
    "@constant",
    "@param",
    "@returns",
    "@return",
    "@property",
    "@prop",
    "@example",
    "@template",
    "@typeParam",
    "@throws",
    "@exception",
    "@type",
    "@module",
    "@author",
    "@since",
    "@typedef",
    "@remarks",
    "@see",
    "@link",
    "@category",
    "@summary",
    "@private",
    "@protected",
    "@public",
    "@internal",
    "@default",
    "@defaultValue",
    "@readonly",
    "@extends",
    "@implements",
    "@override",
    "@deprecated",
    "@ignore",
    "@hidden"
  ],
  "inlineTags": [
    "@link",
    "@inheritDoc",
    "@label"
  ],
  "validation": {
    "invalidLink": true,
    "notDocumented": false
  }
}